; EXERCISE: Modify this function to return the list of 
; sucessor states of s.
;
; This is the top-level next-states (successor) function.
; Some skeleton code is provided below.
; You may delete them totally, depending on your approach.
; 
; If you want to use it, you will need to set 'result' to be 
; the set of states after moving the keeper in each of the 4 directions.
; A pseudo-code for this is:
; 
; ...
; (result (list (try-move s UP) (try-move s DOWN) (try-move s LEFT) (try-move s RIGHT)))
; ...
; 
; You will need to define the function try-move and decide how to represent UP,DOWN,LEFT,RIGHT.
; Any NIL result returned from try-move can be removed by cleanUpList.
; 
;
(defun next-states (s)
  (let* ((pos (getKeeperPosition s 0))
					(x (car pos))
					(y (cadr pos))
					;x and y are now the coordinate of the keeper in s.

					; Use pseudocode in function description.
					(result (list 	(try-move s '(-1 0)) 
													(try-move s '(1 0)) 
													(try-move s '(0 1)) 
													(try-move s '(0 -1))
									)
					)
				)
	 
    (cleanUpList result);end
   );end let
);

; EXERCISE: Modify this function to compute the trivial 
; admissible heuristic.
;
(defun h0 (s) 0)

; EXERCISE: Modify this function to compute the 
; number of misplaced boxes in s.
;
(defun h1 (s)
	(cond
		; Empty state => No boxes
		((null s) 0)

		; Row is finished processing, process next row
		((null (car s)) (h1 (cdr s)))

		; If the first element of the first row is a box, increment and proceed
		((isBox (caar s)) (+ 1 (h1 (cons (cdar s) (cdr s)))))

		; Otherwise, just proceed
		(t (h1 (cons (cdar s) (cdr s)))) 
	)
)

; Function used to get (row, col) positions of all the boxes and goals in the game. 
;
; @param s 				The state of the game
; @param row col 		Helps recursion keep track of current position
; @param boxes goals	Accumulator for the boxes/goals
; @note Ignores boxes already in the goal
(defun get-boxes-and-goals (s row col boxes goals)
	(cond
		; If empty, return the accumulators
		((null s) (list boxes goals))

		; If the row is empty, process the next row
		((null (car s)) (get-boxes-and-goals (cdr s) (+ 1 row) 0 boxes goals))

		; If there is a box, prepend it to boxes and continue processing
		((isBox (caar s))
			(get-boxes-and-goals (cons (cdar s) (cdr s)) row (+ 1 col) (cons (list row col) boxes) goals)
		)

		; If there is a star, prepend it to goals and continue processing
		((or (isStar (caar s)) (isKeeperStar (caar s)))
			(get-boxes-and-goals (cons (cdar s) (cdr s)) row (+ 1 col) boxes (cons (list row col) goals))
		)

		; Otherwise just keep processing
		(t
			(get-boxes-and-goals (cons (cdar s) (cdr s)) row (+ 1 col) boxes goals)			
		)
	)
)

; Calculates the shortest distance a box has to travel to a goal
;
; @param box	The location of the box
; @param goals	A list of the locations of all the goals
; @param min	Accumulator to keep function tail-recursive
(defun min-dist (box goals min)
	; Implementation is a simple loop where we calculate the distance between the box and the first goal,
	; adjust the minimum, and then recurse
	(cond
		((null goals) min)
		(t
			(let ((val (+ (abs (- (car box) (caar goals))) (abs (- (cadr box) (cadar goals))))))
				(min-dist box (cdr goals) (if (< val min) val min))		
			)
		)
	)
)

; Function sums up all minimal Manhattan distances from the boxes to the goals
;
; @param boxes		A list of box locations
; @param goals		A list of goal locations
; @param sum		An accumulator for maintaining the sum and keeping the function tail-recursive
(defun manhattan-dist-sum (boxes goals sum)
	(cond
		((null boxes) sum)
		(t
			(manhattan-dist-sum (cdr boxes) goals (+ sum (min-dist (car boxes) goals 4999)))
		)
	)
)

; EXERCISE: Change the name of this function to h<UID> where
; <UID> is your actual student ID number. Then, modify this 
; function to compute an admissible heuristic value of s. 
; 
; This function will be entered in the competition.
; Objective: make A* solve problems as fast as possible.
; The Lisp 'time' function can be used to measure the 
; running time of a function call.
;
(defun h904606576 (s)
	; First get all box and goal locations
	(let ((boxes-goals (get-boxes-and-goals s 0 0 nil nil)))
		(let ((boxes (car boxes-goals))
			  (goals (cadr boxes-goals))
			 )
			; Sum up the mininimum Manhattan Distances between the boxes and goals and add that with the minimum manhattan distance
			; from the player to the nearest box
			(+ (manhattan-dist-sum boxes goals 0) (if (null boxes) 0 (manhattan-dist-sum (list (rev (getKeeperPosition s 0))) boxes 0)))
		)
	)
)

