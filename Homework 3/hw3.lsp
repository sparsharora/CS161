; Define some global variables
(setq blank 0)
(setq wall 1)
(setq box 2)
(setq keeper 3)
(setq star 4)
(setq boxstar 5)
(setq keeperstar 6)

; Some helper functions for checking the content of a square
(defun isBlank (v)
  (= v blank)
  )

(defun isWall (v)
  (= v wall)
  )

(defun isBox (v)
  (= v box)
  )

(defun isKeeper (v)
  (= v keeper)
  )

(defun isStar (v)
  (= v star)
  )

(defun isBoxStar (v)
  (= v boxstar)
  )

(defun isKeeperStar (v)
  (= v keeperstar)
  )

;
; Helper function of getKeeperPosition
;
(defun getKeeperColumn (r col)
  (cond ((null r) nil)
	(t (if (or (isKeeper (car r)) (isKeeperStar (car r)))
	       col
	     (getKeeperColumn (cdr r) (+ col 1))
	     );end if
	   );end t
	);end cond
  )

;
; getKeeperPosition (s firstRow)
; Returns a list indicating the position of the keeper (c r).
; 
; Assumes that the keeper is in row >= firstRow.
; The top row is the zeroth row.
; The first (right) column is the zeroth column.
;
(defun getKeeperPosition (s row)
  (cond ((null s) nil)
	(t (let ((x (getKeeperColumn (car s) 0)))
	     (if x
		 ;keeper is in this row
		 (list x row)
		 ;otherwise move on
		 (getKeeperPosition (cdr s) (+ row 1))
		 );end if
	       );end let
	 );end t
	);end cond
  );end defun

;
; cleanUpList (l)
; returns l with any NIL element removed.
; For example, if l is '(1 2 NIL 3 NIL), returns '(1 2 3).
;
(defun cleanUpList (L)
  (cond ((null L) nil)
	(t (let ((cur (car L))
		 (res (cleanUpList (cdr L)))
		 )
	     (if cur 
		 (cons cur res)
		  res
		 )
	     );end let
	   );end t
	);end cond
  );end 

; EXERCISE: Modify this function to return true (t)
; if and only if s is a goal state of a Sokoban game.
; (no box is on a non-goal square)
;
; Currently, it always returns NIL. If A* is called with
; this function as the goal testing function, A* will never
; terminate until the whole search space is exhausted.
;
(defun goal-test (s)
  (cond
		; trivially no boxes so game complete
		((null s)	t)

		; if the first entry is a box, game not complete
		((isBox (caar s)) nil)

		; if no more entries in first row, check the other rows
		((equal (cdar s) nil)		(goal-test (cdr s)))

		; otherwise remove first entry and check the rest of the map
		(t (goal-test (cons (cdar s) (cdr s))))
		
	)
);end defun

;; ADDED HELPER FUNCTIONS

; Gets the content located in the (row, col) entry of s
(defun get-content (s row col)
	(if (and (>= row 0) (>= col 0)) (nth col (nth row s)) nil)
)

; Helper function for rev to use an accumulator so that rev is O(N)
(defun rev-append (s accum)
	(cond
		((null s) accum)
		(t (rev-append (cdr s) (cons (car s) accum)))
	)
)

; Used to reverse the list s
(defun rev (s)
	(rev-append s '())
)

; Helper function for sub to make a change to a state
;
; @param row-accum map-accum are accumulators used to to make the function tail-recursive and optimal
(defun substitute-append (s subs row col row-accum map-accum)
	(if (null s) (rev map-accum)
				; Determine whether something should be replaced; if so, calculate what should be there instead
	(let* (		(should-replace (and (not (null subs)) (equal row (caar subs)) (equal col (cadar subs))))
				(sub (if should-replace (cdr subs) subs))
				(val (if should-replace (caddar subs) (caar s)))
			)
			; After substituting, move on to the next element and continue processing the state in row major order
			(if (null (cdar s))
				(substitute-append (cdr s) sub (+ 1 row) 0 '() (cons (rev (cons val row-accum)) map-accum))
				(substitute-append (cons (cdar s) (cdr s)) sub row (+ 1 col) (cons val row-accum) map-accum)
			)
		)
	)
)


; @param subs		Ordered list of (row col val) elements specifying what to
;								change to the state s. Ordered in row-major order.
(defun sub (s subs)
	(substitute-append s subs 0 0 '() '())
)

; @param s		State of the game
; @param dir	A list (x y) where one value is a 0 and the other is either 1 or -1, representing a move in the
;				xy-plane
(defun try-move (s dir)
	(let* ( 		(pos (rev (getKeeperPosition s 0))) 
					; Get position of person, where the person wants to go, and where a box may move as consequence
					(person (get-content s (car pos) (cadr pos)))
					(next   (get-content s (+ (car dir) (car pos)) (+ (cadr dir) (cadr pos))))
					(after  (get-content s (+ (car dir) (car dir) (car pos)) (+ (cadr dir) (cadr dir) (cadr pos))))
				)
		(cond
			; If we try walking into a wall, don't allow it
			((or (null next) (isWall next)) nil)
			
			; If the next space is blank or is a star, we can walk to it
			((or (isBlank next) (isStar next))
				; Get the keeper's new location and old location
				(let ((keeper-new-loc (list (+ (car dir) (car pos)) (+ (cadr dir) (cadr pos)) (if (isStar next) keeperstar keeper)))
							(keeper-old-loc (list (car pos) (cadr pos) (if (isKeeperStar person) star blank)))
							)		
					; Use the substitute function to adjust the map
					(if (or (< (car dir) 0) (< (cadr dir) 0))
						(sub s (list keeper-new-loc keeper-old-loc))
						(sub s (list keeper-old-loc keeper-new-loc))
					)
				)
			)

			; If there is a box and the box is movable, we can move
			((and (or (isBox next) (isBoxStar next)) (not (null after)) (or (isBlank after) (isStar after)))
				; Get the keeper location, where the keepr wants to go, and where the box is to move
				(let (		(keeper-new-loc (list (+ (car dir) (car pos)) (+ (cadr dir) (cadr pos)) (if (isBoxStar next) keeperstar keeper)))
							(keeper-old-loc (list (car pos) (cadr pos) (if (isKeeperStar person) star blank)))
							(box-new-loc (list (+ (car dir) (car dir) (car pos)) (+ (cadr dir) (cadr dir) (cadr pos)) (if (isStar after) boxstar box)))
							)
					; Use the substitute function to adjust the map							
					(if (or (< (car dir) 0) (< (cadr dir) 0))
						(sub s (list box-new-loc keeper-new-loc keeper-old-loc))
						(sub s (list keeper-old-loc keeper-new-loc box-new-loc))
					)
				)
			)

			; Otherwise, we shouldn't move
			(t nil)
		)
	)
)

